<dal>
  <entities>
    <entity id="tp.user">
      <settings/>
      <operations>
        <operation id="getUsersByAccount">
          <columns>
            <column id="ACCOUNTID" type="NUMBER"/>
          </columns>
          <sql>
          SELECT *
            FROM twicepixels.customer_user
           WHERE accountId = ?
          </sql>
        </operation>
      </operations>
    </entity>
  </entities>
</dal>
